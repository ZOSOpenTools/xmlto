export VERSION="0.0.28"
export ZOPEN_TARBALL_URL="https://releases.pagure.org/xmlto/xmlto-$VERSION.tar.gz"
export ZOPEN_TARBALL_DEPS="make zoslib"
export ZOPEN_TYPE="TARBALL"
export ZOPEN_EXTRA_CONFIGURE_OPTS="BASH=\"/bin/env bash\" SED=\"/bin/env sed\" GREP=\"/bin/env grep\""

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to gauge build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
}

zopen_append_to_env()
{
  # echo envars outside of PATH, MANPATH, LIBPATH
}

zopen_append_to_setup()
{
  # echo commands that will run when installing via setup.sh
}

zopen_get_version()
{
  echo "$VERSION"
}
